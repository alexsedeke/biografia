{
  "name": "biografia",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cssnano": "^3.10.0",
    "firebase": "^4.1.3",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.8",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^10.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postcss": "postcss src/**/*.postcss -c ./postcss.config.js --dir src --base src --ext '.css'",
    "postcss-watch": "postcss src/**/*.pcss -c ./postcss.config.js --dir src --base src --ext '.css' --watch",
    "start": "npm-run-all -p postcss-watch start-react",
    "build": "npm run postcss && npm run build-react"
  }
}
